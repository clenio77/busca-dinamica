{
  "version": 2,
  "builds": [
    {
      "src": "busca-dinamica-react/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "busca-dinamica-react/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/busca-dinamica-react/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/busca-dinamica-react/build/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
