{
  "name": "busca-dinamica-react",
  "version": "0.2.0",
  "private": true,
  "description": "Busca dinâmica de endereços (rua, CEP, bairro)",
  "scripts": {
    "start": "npm run build-css && react-scripts start",
    "build": "npm run build-css && node scripts/generate-json-from-db.js && react-scripts build",
    "build-css": "tailwindcss -i ./src/input.css -o ./src/output.css",
    "watch-css": "tailwindcss -i ./src/input.css -o ./src/output.css --watch",
    "test": "react-scripts test",
    "ci": "CI=true react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "echo 'no linter configured'",
    "setup-mcp": "node scripts/setup-mcp.js",
    "mcp-server": "node mcp-servers/cep-scraper-server.js",
    "test-uberlandia": "node scripts/test-uberlandia-scraper.js",
    "scrape-improved": "node scripts/cep-scraper-improved.js",
    "mass-collect": "node scripts/mass-cep-collector.js",
    "sequential-collect": "node scripts/sequential-cep-collector.js",
    "smart-collect": "node scripts/smart-cep-collector.js",
    "ultra-collect": "node scripts/ultra-conservative-collector.js",
    "auto-collect": "node scripts/auto-collector.js",
    "extract-html": "node scripts/extract-html-ceps.js",
    "add-cities": "node scripts/add-other-cities.js",
    "robust-cities": "node scripts/robust-city-collector.js",
    "curated-cities": "node scripts/curated-cities-data.js",
    "setup-vercel": "node scripts/setup-vercel-env.js",
    "weekly-update": "node scripts/weekly-update.js"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "node-cron": "^4.2.1",
    "playwright": "^1.54.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-scripts": "5.0.1",
    "react-window": "1.8.11",
    "sequelize": "^6.37.7",
    "serve": "^14.2.4",
    "sqlite3": "^5.1.7",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.3.1",
    "tailwindcss": "^3.4.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3000"
}
